<p class="lead">Restaurant List</p>
 
<table class="table table-hover table-striped table-condensed">
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Zip Code</th>
    <th>Phone Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
 
  <% @restaurants.each do |restaurant| %>
    <tr>
      <td><%= restaurant.name %></td>
      <td><%= restaurant.description %></td>
      <td><%= restaurant.street %></td>
      <td><%= restaurant.city %></td>
      <td><%= restaurant.state %></td>
      <td><%= restaurant.zipCode %></td>
      <td><%= restaurant.phoneNumber %></td>
      <td><%= link_to 'Show', restaurant_path(restaurant) %></td>
      <td><%= link_to 'Edit', edit_restaurant_path(restaurant) %></td>
      <td><%= link_to 'Delete', restaurant_path(restaurant), method: :delete %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Add Restaurant', new_restaurant_path, class: "btn btn-default" %>
